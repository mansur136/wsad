def spy_game(nums):
    code = [0, 0, 7]
    idx = 0
    for num in nums:
        if num == code[idx]:
            idx += 1
        if idx == len(code):
            return True
    return False

user_input = input("Введите числа через пробел: ")
nums = [int(x) for x in user_input.split()]
print("Результат:", spy_game(nums))